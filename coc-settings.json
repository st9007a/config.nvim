{
  "coc.preferences.rootPatterns": [
    ".flake8",
    ".isort.cfg",
    ".coveragerc",
    ".gitignore",
    ".git",
    ".env",
    ".venv",
    "pytest.ini",
    "Cargo.toml",
    "pyproject.toml",
    "setup.py",
    "setup.cfg"
  ],
  "list.statusLineSegments": null,
  "explorer": {
    "buffer.root.template": "[icon & 1] Buffers",
    "file.root.template": "[icon & 1] Parent ([root])",
    "file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [filename omitCenter 1][modified] [linkIcon & 1][link growRight 1 omitCenter 5]",
    "previewAction.onHover": "labeling",
    "keyMappings.global": {
      ".": "toggleHidden",
      "nf": "addFile",
      "nd": "addDirectory",
      "rm": "delete",
      "mv": "rename",
      "/": "searchRecursive",
      "l": ["wait", "expandable?", "expand", "open"]
    },
    "width": 30
  },
  "python": {
    "analysis": {
      "typeshedPaths": [
        "~/.config/pyright/typeshed"
      ],
      "stubPath": "~/.config/pyright/python-type-stubs"
    },
    "linting": {
      "flake8Enabled": true
    }
  },
  "pyright": {
    "enable": true
  }
}
